{
  "name": "cag",
  "version": "1.0.0",
  "description": "Centr'All Games website built with Payload CMS and Next.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start"
  },
  "dependencies": {
    "@payloadcms/db-sqlite": "3.58.0",
    "@payloadcms/email-nodemailer": "3.58.0",
    "@payloadcms/next": "3.58.0",
    "@payloadcms/payload-cloud": "3.58.0",
    "@payloadcms/richtext-lexical": "3.58.0",
    "@payloadcms/translations": "^3.58.0",
    "@payloadcms/ui": "3.58.0",
    "@tailwindcss/postcss": "^4.1.14",
    "cross-env": "^7.0.3",
    "graphql": "^16.11.0",
    "lucide-react": "^0.546.0",
    "next": "15.3.0",
    "ogl": "^1.0.11",
    "payload": "3.58.0",
    "postcss": "^8.5.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sharp": "0.32.6",
    "tailwindcss": "^4.1.14"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@types/node": "^22.18.8",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.2",
    "eslint": "^9.37.0",
    "eslint-config-next": "15.3.0",
    "prettier": "^3.6.2",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  }
}
